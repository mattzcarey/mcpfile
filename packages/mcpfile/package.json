{
	"name": "mcpfile",
	"version": "0.0.1",
	"description": "alternative mcp client, built for production",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"scripts": {
		"build": "tsdown src/index.ts",
		"typecheck": "tsc --noEmit",
		"dev": "tsdown --watch",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"author": "Matt Carey",
	"license": "MIT",
	"devDependencies": {
		"tsdown": "latest",
		"vitest": "^4.0.7"
	},
	"dependencies": {
		"@mcpfile/schemas": "*",
		"@modelcontextprotocol/sdk": "^1.21.0",
		"effect": "^3.19.1"
	}
}
