{
	"$schema": "../../node_modules/wrangler/config-schema.json",
	"name": "mcpfile-www",
	"compatibility_date": "2025-06-17",
	"compatibility_flags": ["nodejs_compat"],
	"main": "./workers/app.ts",
	"assets": {
		"binding": "ASSETS",
		"not_found_handling": "single-page-application",
		"run_worker_first": ["/api/*", "/docs/*.md", "/docs/*.txt"]
	},
	"observability": {
		"enabled": true
	},
	"routes": [
		{
			"custom_domain": true,
			"pattern": "mcpfile.org"
		}
	]
}
