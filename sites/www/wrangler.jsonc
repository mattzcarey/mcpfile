{
	"$schema": "../../node_modules/wrangler/config-schema.json",
	"name": "mcpfile-www",
	"main": "src/server.ts",
  "compatibility_date": "2025-10-08",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "not_found_handling": "single-page-application",
    "run_worker_first": ["/api/*"]
  },
	"routes": [
		{
			"custom_domain": true,
			"pattern": "mcpfile.org"
		}
	]
}
