{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "mcpfile-www",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-10-08",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "mcpfile.org",
      "custom_domain": true
    }
  ]
}